{
  "name": "gestao-peditz-utils",
  "version": "0.0.6",
  "description": "Gest√£o Peditz",
  "main": "./out/main/index.js",
  "author": "Peditz solucoes",
  "homepage": "www.peditz.com.br",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder",
    "public": "electron-builder -p always",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@phosphor-icons/react": "^2.0.10",
    "antd": "^5.8.3",
    "axios": "^1.4.0",
    "chart.js": "^4.3.3",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "moment": "^2.29.4",
    "numeral": "^2.0.6",
    "react-chartjs-2": "^5.2.0",
    "react-electron-web-view": "^2.0.1",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.15.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/axios": "^0.14.0",
    "@types/chart.js": "^2.9.37",
    "@types/node": "^18.16.19",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.3",
    "electron": "^24.6.2",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.25",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2"
  },
  "build": {
    "extraResources": [
      {
        "from": "build",
        "to": "build"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "peditz-solucoes",
        "repo": "gestao-peditz-utils",
        "private": false
      }
    ],
    "appId": "gestao-peditz-utils.app",
    "mac": {
      "category": "gestao-peditz-utils.app.utils.product"
    }
  }
}
